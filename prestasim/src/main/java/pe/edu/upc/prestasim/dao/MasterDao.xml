<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pe.edu.upc.prestasim.dao.MasterDao">

	<select id="obtainPaymentRanks" resultType="pe.edu.upc.prestasim.beans.RangoIngreso">
		SELECT * FROM rangoingreso
	</select>

	<select id="obtainLoanTypes" resultType="pe.edu.upc.prestasim.beans.TipoPrestamo">
		SELECT * FROM tipoprestamo
	</select>

	<select id="obtainEntityDetails" parameterType="Integer" resultType="pe.edu.upc.prestasim.beans.Entidad">
		SELECT * FROM entidad
		<where>
			identidad = #{identidad, jdbcType=INTEGER}
		</where>
	</select>

</mapper>